{
  "suggest.labelMaxLength": 300,
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "javascript",
    "html",
    "css",
    "vue",
    "markdown"
  ],
  "eslint.autoFixOnSave": true,
	"explorer.file.showHiddenFiles": false,
  "explorer.file.hiddenRules": {
    "filenames": [
      ".bash_history",
      ".npm",
      ".viminfo",
      ".netrwhist"
    ],
    "patternMatches": [
      "\\.swp$"
    ]
  }
}
